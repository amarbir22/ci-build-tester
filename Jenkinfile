pipeline {
  agent any
  tools {
    nodejs 'nodejs'
  }
  stages {
    stage('Get Latest Git Repo') {
      steps {
        // Get some code from a GitHub repository
        git 'https://github.com/amarbir22/ci-build-tester.git'
      }
    }
    stage('Run UI Tests') {
      steps {
        // Get some code from a GitHub repository
        sh 'cd ui-tests && npm run test'
      }
    }
  }
}
